{
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:flowtype/recommended", "@redturtle/eslint-config"],
  "plugins": ["react", "flowtype"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "trailingComma": "all"
      }
    ],
    "no-console": "off"
  }
}
